<!DOCTYPE html>

<title>Event</title>

{% block body %}
    <h1>Event</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ event.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ event.label }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ event.number }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ event.value }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('back_event_index') }}">back to list</a>

    <a href="{{ path('back_event_edit', {'id': event.id}) }}">edit</a>

    {{ include('back/event/_delete_form.html.twig') }}
{% endblock %}
